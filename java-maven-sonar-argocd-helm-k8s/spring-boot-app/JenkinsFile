pipeline {
agent any
tools {
          maven "Maven3"
     }
environments {
          SONARQUBE_ENV = "mysonarqube"
}
stages {
stage('Checkout') {
     steps {
     git branch: 'main', url: 'https://github.com/manikiran7/Jenkins-Zero-To-Hero.git'
     }
}

stage('Code Qulaity Check') {
     steps {
               withSonarQubeEnv("${"SONARQUBE_ENV"}) {
                    sh 'mvn clean verify sonar:soanr'
                    }
               }
          }
     }
}          




